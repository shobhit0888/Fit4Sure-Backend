<%- include("partials/header.ejs") %>

<section class="container-fluid plans-section">
  <p class="plans-heading"><%= web_service_title %></p>
  <p class="plans-heading-bottom"><%= web_service_description %></p>
  <div class="container blank-container2">
    <div class="row">
      <div class="col-md-2">
        <div class="service-box-container">
          <% web_services.forEach(web_services => { %>
          <a href="/website/service-details/<%= web_services._id %>" class="text-white">
            <div class="service-box">
              <div class="empty-box">
                <img
                  src="/uploads/Web_services/<%= web_services.image %>"
                  alt="empty-box"
                  class="empty-box-img"
                  width="208px"
                  height="208px"
                  style="border-radius: 19.8246px"
                />
              </div>
              <p><%= web_services.title ? web_services.title : "" %></p>
            </div>
          </a>
          <% }) %>
        </div>
      </div>
    </div>
  </div>
</section>



<%- include("partials/footer.ejs") %>
